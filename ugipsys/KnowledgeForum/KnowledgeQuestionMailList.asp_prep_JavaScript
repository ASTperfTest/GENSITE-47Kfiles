 function RedirectToMail()
 {
  window.location.href = "/KnowledgeForum/KnowledgeQuestionMail.asp?questionId=Cx_0dummy&nowPage=Cx_0dummy&pagesize=Cx_0dummy";
 }
 function RedirectReply( qid, did, rid, eid)
 {
  window.open("Cx_0dummy/Knowledge/KnowledgeExpertReply.aspx?ArticleId=" + qid + "&DArticleId=" + did + "&rand=" + rid + "&expertId=" + eid);
 }










 