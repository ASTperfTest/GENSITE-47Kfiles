                              function onClick___151089436() {formDelete()}
                             function onClick___1657291224() {formReset()}
                             function onClick___1352909932() {formBack()}




 function formEdit() {
  document.getElementById("submitTask").value = "edit";
  if ( document.getElementById("xImportant").value = "" ) {
   document.getElementById("xImportant").value = "0";
  }
  document.getElementById("reg").submit();
 }
 function formDelete() {
  document.getElementById("submitTask").value = "delete";
  document.getElementById("reg").submit();
 }
 function formReset() {
  document.getElementById("submitTask").value = "reset";
  document.getElementById("reg").submit();
 }
 function formBack() {
  window.location.href = "/KnowledgeForum/KnowledgeDiscussList.asp?questionId=Cx_0dummy";
 }








 