                                                function onclick___37515954() {addNode()}







                                   function onClick___1283611761() {formCancel()}
                               function onClick___1657291224() {formReset()}
                               function onClick___1283611761() {formCancel()}










 function subjectListOnChange()
 {
  var subjectId = document.getElementById("subjectList").options[document.getElementById("subjectList").selectedIndex].value;
  window.location.href = "publish_ctNodeset.asp?type=Cx_0dummy&subjectId=" + subjectId;
 }
 function addNode()
 {
  var subjectId = document.getElementById("subjectList").options[document.getElementById("subjectList").selectedIndex].value;
  var ctNodeId = document.getElementById("ctNodeList").options[document.getElementById("ctNodeList").selectedIndex].value;  
  window.location.href = "publish_ctNodeset.asp?type=Cx_0dummy&add=Y&subjectId=" + subjectId + "&ctNodeId=" + ctNodeId;
 }
 function delNode(id)
 {  
  window.location.href = "publish_ctNodeset.asp?type=Cx_0dummy&del=Y&id=" + id;
 }
 function formReset(id)
 {  
  window.location.href = "publish_ctNodeset.asp?type=Cx_0dummy";
 }
 function formCancel(id)
 {  
  window.location.href = "keywords_set.asp";
 }








 