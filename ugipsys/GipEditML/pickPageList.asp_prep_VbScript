






































































































































































     sub gopage_onchange      
           newpage=reg.gopage.value     
           document.location.href="Cx_0dummylist.asp?keep=y&nowpage=" & newpage & "&pagesize=Cx_0dummy"    
     end sub      
     
     sub perpage_onchange                
           newperpage=reg.perpage.value
           document.location.href="Cx_0dummylist.asp?keep=y&nowpage=Cx_0dummy" & "&pagesize=" & newperpage                    
     end sub 
     
     sub rtpick(xi, xtitle)
  xmlstr = "<?xml version=""1.0"" encoding=""utf-8"" ?>" & cvbcrlf
  xmlstr = xmlstr & "<rtlist></rtlist>" & cvbcrlf
      window.opener.Cx_0dummy xmlstr
      window.close
     end sub

 