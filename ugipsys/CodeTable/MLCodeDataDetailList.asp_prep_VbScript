     sub gopage_onchange      
           newpage=reg.gopage.value     
           document.location.href="Cx_0dummylist.asp?codeid=Cx_0dummy&codename=Cx_0dummy&nowpage=" & newpage & "&strsql=Cx_0dummy" & "&pagesize=Cx_0dummy"    
     end sub      
     
     sub perpage_onchange                
           newperpage=reg.perpage.value
           document.location.href="Cx_0dummylist.asp?codeid=Cx_0dummy&codename=Cx_0dummy&nowpage=Cx_0dummy" & "&strsql=Cx_0dummy" & "&pagesize=" & newperpage                    
     end sub 
     sub changeml_onchange
'      msgbox reg.changeml.value
      document.location.href="mlCx_0dummylist.asp?codeid=Cx_0dummy&codename=Cx_0dummy&ml=" & reg.changeml.value
     end sub

 