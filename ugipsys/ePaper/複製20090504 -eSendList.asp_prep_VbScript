


































































































































































  dim gpkey
     sub gopage_onchange      
           newpage=reg.gopage.value     
           document.location.href="Cx_0dummylist.asp?epubid=Cx_0dummy&nowpage=" & newpage & "&pagesize=Cx_0dummy"    
     end sub      
     
     sub perpage_onchange                
           newperpage=reg.perpage.value
           document.location.href="Cx_0dummylist.asp?epubid=Cx_0dummy&nowpage=Cx_0dummy&pagesize=" & newperpage                    
     end sub 

     sub setpkey(xv)
      gpkey = xv
     end sub



 