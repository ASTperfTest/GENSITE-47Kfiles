'     window.history.back
 <script language=vbs></script>     </body>       dim gpkey
     sub gopage_onchange      
           newpage=reg.gopage.value     
           document.location.href="Cx_0dummylist.asp?nowpage=" & newpage & "&strsql=" & strsql & "&pagesize=Cx_0dummy"    
     end sub      
     
     sub perpage_onchange                
           newperpage=reg.perpage.value
           document.location.href="Cx_0dummylist.asp?nowpage=Cx_0dummy" & "&strsql=Cx_0dummy&pagesize=" & newperpage                    
     end sub 

     sub setpkey(xv)
      gpkey = xv
     end sub

sub getxmlname()
 xname = inputbox("請輸入匯入之xml檔全名:") 
 if xname="" then exit sub
 window.navigate "basedsdfieldimportxml.asp?Cx_0dummy&xmlfile="& escape(xname)
end sub
