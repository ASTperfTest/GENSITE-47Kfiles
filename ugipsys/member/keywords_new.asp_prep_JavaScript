 function formSubmit() 
 {
  if(document.getElementById("keyword").value == "" || document.getElementById("keyword").value == "請輸入關鍵字" ) {
   alert("請輸入關鍵字");
  }
  else {
   document.getElementById("iForm").submit();
  }  
  }
 function formReset() 
 {
  window.location.href = "keywords_new.asp?memberId=Cx_0dummy&nowPage=Cx_0dummy&pagesize=Cx_0dummy";
  }
 function formCancel() 
 {
  window.location.href = "keywords_set.asp?memberId=Cx_0dummy&nowPage=Cx_0dummy&pagesize=Cx_0dummy";
  }








 