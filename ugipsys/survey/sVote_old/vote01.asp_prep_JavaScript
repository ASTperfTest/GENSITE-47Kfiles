


























                               
          
function send() {
 var form = document.post;
 if ( form.name.value == "" ) {
  alert("請輸入姓名！");
  form.name.focus();
  return false;
 } else if ( form.email.value == "" ) {
  alert("請輸入email！");
  form.email.focus();
  return false;
 }

 return true;
}
     





                                                                             function onSubmit___1912918050() {return send();}






















































































































































































































































































 