


















































                               
          
function send() {
 var form = document.post;

 if ( form.m014_name.value == "") {
  alert("您忘了填寫姓名了！");
  form.m014_name.focus();
  return false;
 }

 if ( form.e_mail.value == "" ) {
  alert("您忘了填寫email了！");
  form.e_mail.focus();
  return false;
 }

 return true;
}
     






                                                                             function onSubmit____516022209() {return send()}




























































































































































































































































































 