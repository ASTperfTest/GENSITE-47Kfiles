





































                                                      
          
function CheckValue() {
 if ( document.form1.subject.value == '' ) {
  alert('請輸入主題名稱！');
  document.form1.subject.focus();
  return false;
 }
 
 return true;
}
     



                                  function onSubmit___1715157505() {return CheckValue();}





































































































