


















































                               
          
function send1() {
 var form = document.post;

 if ( form.m014_A.value == "") {
  alert("您忘了填寫陳情的時間了！");
  form.m014_A.focus();
  return false;
 }

 if ( form.e_mail.B == "" ) {
  alert("您忘了填寫機關收發文號了！");
  form.e_B.focus();
  return false;
 }

 if ( form.e_mail.C == "" ) {
  alert("您忘了填寫陳請事由了！");
  form.C.focus();
  return false;
 }
 return true;
}

function send() {
 var form = document.post;

 if ( form.m014_name.value == "") {
  alert("您忘了填寫姓名了！");
  form.m014_name.focus();
  return false;
 }

 if ( form.e_mail.value == "" ) {
  alert("您忘了填寫email了！");
  form.e_mail.focus();
  return false;
 }

 return true;
}
     









