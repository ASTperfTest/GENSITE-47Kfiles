






































































































































































































































































 
  sub formmodsubmit()
   reg.submittask.value = "delete"
   reg.action = "Cx_0dummylist.asp?icuitem=Cx_0dummy"
   reg.submit
  end sub
  
  dim gpkey
    sub gopage_onchange      
           newpage=reg.gopage.value     
           document.location.href="Cx_0dummylist.asp?keep=y&nowpage=" & newpage & "&pagesize=Cx_0dummy"    
     end sub      
     
     sub perpage_onchange                
           newperpage=reg.perpage.value
           document.location.href="Cx_0dummylist.asp?keep=y&nowpage=Cx_0dummy" & "&pagesize=" & newperpage                    
     end sub 

     sub shortlonglist(sorl)
           document.location.href="Cx_0dummylist.asp?keep=y&nowpage=Cx_0dummy&pagesize=Cx_0dummy" _
   & "&shortlonglist=" & sorl
     end sub         
 <script language=vbs></script>Cx_0dummy 
 