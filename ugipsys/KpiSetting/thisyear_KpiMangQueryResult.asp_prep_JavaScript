 function gradetypeonchange()
 {  
  window.location.href = "thisyear_KpiMangQueryResult.asp?keep=Y&gradetypethis=" + document.getElementById("gradetypethis").value + "&nowPage=Cx_0dummy&pagesize=Cx_0dummy";
 }
