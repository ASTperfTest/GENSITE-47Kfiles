function chgdate(year, month, day)
 dim newmonth, newday

 if cint(month) < 10 then
  newmonth = "0" & month
 else
  newmonth = month
 end if

 if cint(day) < 10 then
  newday = "0" & day
 else
  newday = day
 end if

 chgdate = year & "/" & newmonth & "/" & newday

end function







































































  
