        function Slide_Interval(s) {
            mySlide_interval += s;
            mySlide_interval = Math.max(1000, mySlide_interval);
            mySlide_interval = Math.min(5000, mySlide_interval);
            switch (mySlide_interval) {
                case 1000:
                    $("#speedTitle").text("��");
                    break;
                case 3000:
                    $("#speedTitle").text("��");
                    break;
                case 5000:
                    $("#speedTitle").text("�C");
                    break;
            }
            $('#nextthumb').click();
        }
    













































































